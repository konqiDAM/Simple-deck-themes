{
	"name": "Animated Synthwave",
	"version": "v1.0",
	"author": "Konqi",
	"target": "Keyboard",
	"manifest_version": 2,
	"description": "Animated synthwave keyboard",
	"inject": {
		"shared.css": ["SP"]
	},
	"patches": {
		"Keyboard primary letters transparency": {
			"default": "75",
			"type": "slider",
			"values": {
				"0": {
					"transparencyMain/0.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"15": {
					"transparencyMain/15.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"30": {
					"transparencyMain/30.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"45": {
					"transparencyMain/45.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"60": {
					"transparencyMain/60.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"75": {
					"transparencyMain/75.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"90": {
					"transparencyMain/90.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"100": {
					"transparencyMain/100.css": ["SP", "MainMenu", "QuickAccess"]
				}
			}
		},
		"Keyboard secondary keys transparency": {
			"default": "75",
			"type": "slider",
			"values": {
				"0": {
					"transparencyAux/0.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"15": {
					"transparencyAux/15.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"30": {
					"transparencyAux/30.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"45": {
					"transparencyAux/45.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"60": {
					"transparencyAux/60.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"75": {
					"transparencyAux/75.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"90": {
					"transparencyAux/90.css": ["SP", "MainMenu", "QuickAccess"]
				},
				"100": {
					"transparencyAux/100.css": ["SP", "MainMenu", "QuickAccess"]
				}
			}
		},
		"Keyboard background animation": {
			"default": "Carwave",
			"type": "dropdown",
			"values": {
				"Carwave": {},
				"Sunywave": {
					"keyboard/sunywave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Planetwave": {
					"keyboard/planetwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Matrixwave": {
					"keyboard/matrixwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Matrix": {
					"keyboard/matrix.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"LoopingBall": {
					"keyboard/loopingball.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Hellwave": {
					"keyboard/hellwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Sheet": {
					"keyboard/sheet.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Palmwave": {
					"keyboard/palmwave.css": ["QuickAccess", "SP", "MainMenu"]
				}
			}
		},
		"Home background animation": {
			"default": "LoopingBall",
			"type": "dropdown",
			"values": {
				"LoopingBall": {},
				"Hellwave": {
					"background/hellwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Matrix": {
					"background/matrix.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Islandwave": {
					"background/islandwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Vaporwave": {
					"background/vaporwave.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Car colorfull": {
					"background/car2.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Car regular": {
					"background/car.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Sheet": {
					"background/sheet.css": ["QuickAccess", "SP", "MainMenu"]
				},
				"Palmwave": {
					"background/palmwave.css": ["QuickAccess", "SP", "MainMenu"]
				}
			}
		}
	}
}